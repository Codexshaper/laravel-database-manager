{
    "name": "codexshaper/laravel-database-manager",
    "description": "Laravel Database Manager",
    "keywords": [
        "dbm",
        "databse",
        "CRUD",
        "BREAD",
        "mysql",
        "sqlite",
        "pgsql",
        "mongodb",
        "sqlsrv",
        "laravel",
        "vuejs2",
        "databse-manager",
        "laravel-databse-manager",
        "dbm-manager",
        "dumper",
        "backup",
        "restore",
        "api",
        "api-authentication",
        "passport"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/Codexshaper/laravel-database-manager",
    "authors": [
        {
            "name": "Md Abu Ahsan Basir",
            "email": "maab.career@gmail.com",
            "homepage": "https://github.com/maab16"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0|^7.0",
        "doctrine/dbal": "^2.10",
        "laravel/passport": "^8.0",
        "codexshaper/database-backup-restore": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "CodexShaper\\DBM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodexShaper\\DBM\\": "src/",
            "CodexShaper\\DBM\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodexShaper\\DBM\\ManagerServiceProvider"
            ],
            "aliases": {
                "DBM": "CodexShaper\\DBM\\Facades\\Manager"
            }
        }
    }
}
